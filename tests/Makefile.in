
@SET_MAKE@

TESTDIRS= @TESTDIRS@

srcdir= @srcdir@

all: check

check: 
	for dir in $(TESTDIRS) ; do \
       (cd "$$dir" && $(MAKE) check ) || echo Test $$dir failed ; \
    done

distclean: clean
	for dir in $(TESTDIRS) ; do \
       if test -f "$$dir/Makefile" ; then \
          (cd "$$dir" && $(MAKE) distclean ) ; \
       fi \
    done
	rm -f config.h config.status config.log Makefile

clean:
	for dir in $(TESTDIRS) ; do \
       if test -f "$$dir/Makefile" ; then \
           (cd "$$dir" && $(MAKE) clean ) ; \
       fi \
    done
